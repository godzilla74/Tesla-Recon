<script>
  // this uses an out-of-the-box jQuery easing function.
  $(document).ready(function() {
    $('.tsla-jump').on('click', function(e) {
      if (this.hash !== "") {
        e.preventDefault();
        var hash = this.hash;
        $('html, body').animate({
          scrollTop: $(hash).offset().top
        }, 300, function(){});
      }
    });
  });
</script>  </div>

  

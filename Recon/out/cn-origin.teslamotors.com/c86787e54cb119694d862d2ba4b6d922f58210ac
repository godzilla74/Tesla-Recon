                    <!-- Google Analytics -->
            <script>
                (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');  // CONFIG: List all query parameters to remove from the URL here
                var piiParams = ['name', 'firstname', 'lastname', 'nickname', 'address', 'gender', 'p', 'e', 'profileurl', 'email', 'pwd', 'fname', 'lname', 'user', 'location', 'query'];  // CONFIG: Text to use whe replacing email addressed and parameters to be removed
                var emailReplaceText = 'email_removed';
                var paramReplaceText = '';  // Accepts string, returns same with anything that looks like an email address replaced with replaceText
                var cleanEmail = function(str) {
                    if (!str) {
                        return '';
                    }
                    return str.replace(/([a-zA-Z0-9\.\+_-`~!#\$%\^&*\(\)]+(@|%40|%2540)[a-zA-Z0-9\.\+_-`~!#\$%\^&*\(\)]+\.[a-zA-Z0-9\.\+_-`~!#\$%\^*\(\)]+)/gi, emailReplaceText);
                };

                // Accepts string with key value pairs, like a query string, and returns same with pairs removed if the key is on the list of PII keys
                var cleanParams = function(qs, replaceText) {
                    replaceText = replaceText || '';
                    if (!qs) {
                        return '';
                    }
                    var cleanedQS = '', queryArray = qs.split(/([&;#\?])/);
                    for (var i=0,len=queryArray.length; i<len; i++) {
                        var name = queryArray[i].split('=')[0];
                        if (piiParams.includes(name)) {
                            //If nothing is placed for replaceText skip the trailing &.
                            if (replaceText === '') {
                                i++
                                continue
                            }
                            cleanedQS += replaceText;
                        } else {
                            cleanedQS += queryArray[i];
                        }
                    }
                    //Add back ? if a query string still exists.
                    if (cleanedQS !== '')
                        cleanedQS = '?' + cleanedQS
                    return cleanEmail(cleanedQS);
                };

                // Accepts full URL or URI stem and returns same with cleaned query string and/or hash identifier
                var cleanPagePath = function(pagePath, emailRemovedText) {
                    if (!pagePath) {
                        return '';
                    }
                    var urlSplit = pagePath.split(/[\?\#]/);

                    return urlSplit[0] + cleanParams(urlSplit[1], emailRemovedText);
                };
                ga('create', 'UA-9152935-1', 'auto', {allowLinker: true});
                ga('require', 'ec');
                ga('set', 'currencyCode', 'CNY'); // Set tracker currency to CNY
                ga('set', 'displayFeaturesTask', null);
                ga('set', 'location', cleanPagePath(document.location.href));
                ga('set', 'referrer', cleanPagePath(document.referrer));
                ga('send', 'pageview');
            </script>
            <!-- End Google Analytics -->
            </head>

